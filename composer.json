{
  "name": "paytic/omnipay-simplify",
  "type": "library",
  "license": "MIT",
  "description": "Mastercard Simplify driver for the Omnipay PHP library\n\n",
  "keywords": [
    "simplify",
    "mastercard",
    "driver",
    "gateway",
    "merchant",
    "omnipay",
    "pay",
    "payment"
  ],
  "homepage": "https://github.com/paytic/omnipay-simplify",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/bytic/omnipay-tests"
    }
  ],
  "autoload": {
    "psr-4": {
      "Paytic\\Omnipay\\Simplify\\": "src/"
    },
    "classmap": [
      "lib"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Paytic\\Omnipay\\Simplify\\Tests\\Fixtures\\": "tests/fixtures",
      "Paytic\\Omnipay\\Simplify\\Tests\\": "tests/src"
    }
  },
  "require": {
    "ext-json": "*",
    "bytic/omnipay-common": "^3.0"
  },
  "require-dev": {
    "omnipay/tests": "dev-master",
    "phpunit/phpunit": "^8.0|^9.0",
    "mockery/mockery": "^1.0",
    "vlucas/phpdotenv": "^4.0",
    "php-http/mock-client": "^1.1",
    "php-http/guzzle6-adapter": "^1.1.1|^2"
  }
}
